<?xml version="1.0" encoding="UTF-8"?>
<!--
  Template focado em "Surface only" (sem canais/neve/solo/lençol/etc.).
  Observação:
	- Define valores numéricos neutros/baixos para várias chaves exigidas
		pelos módulos, com intuito de evitar erros de validação (initonly).
  - NÃO fornece mapas (Ldd, Channels, LAI, mapas de solo...), portanto
    estes módulos continuam efetivamente inativos/sem dados.
  - O app ainda patcha DEM, MaskMap, datas, timestep e PathOut.
-->
<lisflood>
	<lfuser/>
	<lfbinding>
		<!-- SurfaceRouting (defaults neutros) -->
		<textvar name="OFOtherInitValue" value="0"/>
		<textvar name="OFForestInitValue" value="0"/>
		<textvar name="OFDirectInitValue" value="0"/>
		<textvar name="Grad" value="0.0001"/>
		<textvar name="GradMin" value="0.0001"/>
		<textvar name="OFDepRef" value="0.001"/>
		
		<!-- MiscInitial -->
		<textvar name="DtSecChannel" value="0"/>
		<textvar name="GwLoss" value="0"/>
		<textvar name="GwPercValue" value="0"/>
		<textvar name="PrScaling" value="1"/>
		<textvar name="CalEvaporation" value="0"/>

		<!-- Snow (desligado via zeros) -->
		<textvar name="ElevationStD" value="0"/>
		<textvar name="TemperatureLapseRate" value="0"/>
		<textvar name="SnowSeasonAdj" value="0"/>
		<textvar name="TempSnow" value="0"/>
		<textvar name="SnowFactor" value="0"/>
		<textvar name="SnowMeltCoef" value="0"/>
		<textvar name="TempMelt" value="0"/>
		<textvar name="SnowCoverAInitValue" value="0"/>
		<textvar name="SnowCoverBInitValue" value="0"/>
		<textvar name="SnowCoverCInitValue" value="0"/>

		<!-- GroundWater (zeros) -->
		<textvar name="UpperZoneTimeConstant" value="0"/>
		<textvar name="LowerZoneTimeConstant" value="0"/>
		<textvar name="LZInitValue" value="0"/>
		<textvar name="LZThreshold" value="0"/>
		<textvar name="UZInitValue" value="0"/>
		<textvar name="UZForestInitValue" value="0"/>
		<textvar name="UZIrrigationInitValue" value="0"/>

		<!-- Soil (apenas parâmetros escalares) -->
		<textvar name="CourantCrit" value="0"/>
		<textvar name="LeafDrainageTimeConstant" value="0"/>
		<textvar name="AvWaterRateThreshold" value="0"/>
		<textvar name="b_Xinanjiang" value="0"/>
		<textvar name="PowerPrefFlow" value="0"/>
		<textvar name="DSLRInitValue" value="0"/>
		<textvar name="DSLRForestInitValue" value="0"/>
		<textvar name="DSLRIrrigationInitValue" value="0"/>
		<textvar name="CumIntInitValue" value="0"/>
		<textvar name="CumIntForestInitValue" value="0"/>
		<textvar name="CumIntIrrigationInitValue" value="0"/>
		<textvar name="CumIntSealedInitValue" value="0"/>
		<textvar name="SMaxSealed" value="0"/>
	</lfbinding>
	<lfoptions/>
</lisflood>
